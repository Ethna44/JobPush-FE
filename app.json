{
  "expo": {
    "name": "JobPush",
    "slug": "JobPush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoJobPush-Photoroom.jpg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logoJobPush-Photoroom.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2B3033"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoJobPush-Photoroom.jpg",
        "backgroundColor": "#2B3033"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
